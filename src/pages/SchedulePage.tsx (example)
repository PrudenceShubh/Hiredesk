import { useUser } from '../contexts/UserContext';

export const SchedulePage = () => {
  const { user, isLoading } = useUser();

  if (isLoading) {
    return <div>Loading...</div>;
  }

  if (!user) {
    return <div>Please log in</div>;
  }

  return (
    <div>
      <h1>Schedule for {user.name}</h1>
      {/* Your schedule logic */}
    </div>
  );
};
